(version 6.0.1)

(build
 (progn
  (when
   %{pkg-self:dev}
   (run dune subst))
  (run dune build -p %{pkg-self:name} -j %{jobs} @install)))

(depends
 dune
 ocaml
 ppxlib
 cmdliner
 sedlex
 menhir
 menhirLib
 menhirSdk
 yojson
 ocamlfind)

(source
 (fetch
  (url
   https://github.com/ocsigen/js_of_ocaml/releases/download/6.0.1/js_of_ocaml-6.0.1.tbz)
  (checksum
   sha256=813dbee2b62e1541049ea23a20e405cf244e27ebfa9859785cfa53e286d2c614)))
