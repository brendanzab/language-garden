let fst [A, B] : (A, B) -> A := fun (p : (A, B)) => p.0;
let snd [A, B] : (A, B) -> B := fun (p : (A, B)) => p.1;
let elim [A, B, C] : (A, B) -> (A -> B -> C) -> C :=
  fun (p : (A, B)) => fun (f : A -> B -> C) => f p.0 p.1;
let curry [A, B, C] : ((A, B) -> C) -> A -> B -> C :=
  fun (f : (A, B) -> C) => fun (a : A) => fun (b : B) => f (a, b);
let uncurry [A, B, C] : (A -> B -> C) -> (A, B) -> C :=
  fun (f : A -> B -> C) => fun (p : (A, B)) => f p.0 p.1;
() : ()
