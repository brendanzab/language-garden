let apply : [decr : Int | incr : Int | square : Int] -> Int :=
  fun (x : [decr : Int | incr : Int | square : Int]) =>
    match x with
    | [decr := x] => #int-sub x 1
    | [incr := x] => #int-add x 1
    | [square := x] => #int-mul x x
    end;
apply ([incr := 1] : [decr : Int | incr : Int | square : Int]) : Int
